<?xml version='1.0'?>

<robot name="myrobot" xmlns:xacro="http://www.ros.org/wiki/xacro">



  <xacro:include filename="$(find racer_description)/urdf/racer.gazebo" />
  <xacro:include filename="$(find racer_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find racer_description)/urdf/macros.xacro" />
  <xacro:property name="cameraSize" value="0.05"/>
  <xacro:property name="cameraMass" value="0.1"/>


  <!--  body  -->
  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>
       
    <inertial>
      <mass value="10.0"/>
      <origin xyz="0.0 0 0.1" rpy=" 0 0 0"/>
      <inertia
          ixx="0.5" ixy="0" ixz="0"
          iyy="1.0" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <geometry>
        <box size=".49 .18 .1"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".49 .18 .1"/>
      </geometry>
    </visual>
  </link>


  <!--  wheels  -->
  <link name="left_wheel_front">
    <collision name="collision">
      <origin xyz="0 0.02 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.045"/>
      </geometry>
    </collision>
    <visual name="left_wheel_front_visual">
      <origin xyz="0 0.02 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.045"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0.02 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.05" h="0.045"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
  </link>

  <link name="left_wheel_back">
    <collision name="collision">
      <origin xyz="0 0.02 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.045"/>
      </geometry>
    </collision>
    <visual name="left_wheel_back_visual">
      <origin xyz="0 0.02 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.045"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0.02 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.05" h="0.045"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
  </link>

  <link name="right_wheel_front">
    <collision name="collision">
      <origin xyz="0 -0.02 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.045"/>
      </geometry>
    </collision>
    <visual name="right_wheel_front_visual">
      <origin xyz="0 -0.02 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.045"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 -0.02 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.05" h="0.045"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
  </link>

  <link name="right_wheel_back">
    <collision name="collision">
      <origin xyz="0 -0.02 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.045"/>
      </geometry>
    </collision>
    <visual name="right_wheel_back_visual">
      <origin xyz="0 -0.02 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.045"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 -0.02 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.05" h="0.045"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
  </link>

  <joint type="continuous" name="left_wheel_front_hinge">
    <origin xyz="0.16 0.095 -0.05" rpy="0 0 0"/>
    <child link="left_wheel_front"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>

  <joint type="continuous" name="left_wheel_back_hinge">
    <origin xyz="-0.16 0.095 -0.05" rpy="0 0 0"/>
    <child link="left_wheel_back"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_front_hinge">
    <origin xyz="0.16 -0.095 -0.05" rpy="0 0 0"/>
    <child link="right_wheel_front"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_back_hinge">
    <origin xyz="-0.16 -0.095 -0.05" rpy="0 0 0"/>
    <child link="right_wheel_back"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>


  <!-- Camera -->
  <link name="camera">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="${cameraSize} ${cameraSize} ${cameraSize}"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="${cameraSize} ${cameraSize} ${cameraSize}"/>
      </geometry>
      <material name="green"/>
    </visual>

    <inertial>
      <mass value="${cameraMass}" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <box_inertia m="${cameraMass}" x="${cameraSize}" y="${cameraSize}" z="${cameraSize}" />
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>

  <joint name="camera_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz=".2 0 0.5" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="camera"/>
  </joint>


  <joint name="hokuyo_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz=".15 0 .1" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="hokuyo"/>
  </joint>

  <!-- Hokuyo Laser -->
  <link name="hokuyo">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
    <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://racer_description/meshes/hokuyo.dae"/>
      </geometry>
    </visual>

    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>

</robot>
